<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.selferral.admin.api.mapper.ExchangeListMapper">


	<!-- 거래소 목록 조회 -->
	<select id="getExchangeList" resultType="io.selferral.admin.api.model.dto.ExchangeDto">
		SELECT 
			exchange_no,
			en_name
		  FROM exchange
		 ORDER BY priority ASC	
	</select>




</mapper>